<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="default">
  <Description/>
  <FaultRules/>
  <PreFlow name="PreFlow">
    <Request>
      <Step>
        <Name>AM-SetAudience</Name>
      </Step>
    </Request>
    <Response/>
  </PreFlow>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <Flows/>
  <HTTPTargetConnection>
    <Properties/>
    <LoadBalancer>
      <Server name="cloud-function"/>
    </LoadBalancer>
    <Path>/anything</Path>
    <!-- <URL>https://httpbin-418182175683.australia-southeast1.run.app</URL> -->
    <Authentication>
      <GoogleIDToken>
        <!-- <Audience>https://httpbin-418182175683.australia-southeast1.run.app/anything</Audience> -->
        <!-- <Audience>https://httpbin-418182175683.australia-southeast1.run.app</Audience> -->
        <!-- <Audience useTargetUrl="true"/> -->
        <Audience ref="local.audience"/>
      </GoogleIDToken>
    </Authentication>
  </HTTPTargetConnection>
</TargetEndpoint>
