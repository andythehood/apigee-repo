<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<LookupCache continueOnError="false" enabled="true" name="LC-Lookup-AccessID">
  <DisplayName>LC-Lookup-AccessID</DisplayName>
  <CacheResource>authentication_cache</CacheResource>
  <CacheKey>
    <Prefix>custom_key_validation</Prefix>
    <KeyFragment ref="request.header.accessID"/>
    <KeyFragment ref="request.header.siteID"/>
  </CacheKey>
  <Scope>Exclusive</Scope>
  <AssignTo>cache.hit.flag</AssignTo>
</LookupCache>
